{
    "version": 2,
    "framework": "laravel",
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@0.6.0"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(css|js|images|fonts)/(.*)",
            "dest": "/public/$1/$2",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Bright Smart Service",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://bright-smart-service.vercel.app",
        "DB_CONNECTION": "pgsql",
        "DB_HOST": "db.oplrqwcuigdmynrktiwm.supabase.co",
        "DB_PORT": "5432",
        "DB_DATABASE": "postgres",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "array",
        "QUEUE_DRIVER": "sync",
        "LOG_CHANNEL": "errorlog",
        "ASSET_URL": "https://bright-smart-service.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "SSR_TEMP_PATH": "/tmp/ssr"
    }
}