{
    "version": 2,
    "framework": null,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images|fonts|build)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Bright Smart Service",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://bright-smart-service.vercel.app",
        "DB_CONNECTION": "pgsql",
        "DB_HOST": "db.oplrqwcuigdmynrktiwm.supabase.co",
        "DB_PORT": "5432",
        "DB_DATABASE": "postgres",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "array",
        "QUEUE_DRIVER": "sync"
    }
}